@model List<Hangman.Web.Models.PlayersModel>

<div class="well">
    <div>Top scores:</div>
    @{
        if (Model != null)
        {
            for (int i = 0; i < Model.Count; i++)
            {
                <div>
                    <span>@(i + 1).</span>
                    <span>@Model[i].Name :</span>
                    <span>@Model[i].Score</span>
                </div>
            }
        }
    }
</div>

